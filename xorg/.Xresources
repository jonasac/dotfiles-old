! URxvt settings ---------------------------------------------------------------

URxvt.font: xft:Liberation Mono:pixelsize=12:antialias=true:hinting=true
URxvt.boldFont: xft:Liberation Mono:bold:pixelsize=12:antialias=true:hinting=true

! Less space between letters
URxvt.letterSpace: -1

! Disable scrollbar
URxvt.scrollBar: false

! Extensions
URxvt.perl-ext-common: default,matcher,selection-to-clipboard

! Clickable URLs
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

! Transparency
URxvt*.transparent: true
URxvt*.shading: 30
! jellybeans theme

*.background: #121212
*.foreground: #d7d7d7
*.cursorColor: #d7d7d7

